<!DOCTYPE html>
<html>
<head>
    <title>Parking</title>
    <%= favicon_link_tag 'favicon/favicon.ico' %>
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>

</head>
<body class="only_header">
    <header>
        <div class="content">
            <%= render 'layouts/parts/header' %>
        </div>
    </header>

    <div class="main">
        <div class="content">
            <%= yield %>
        </div>
    </div>

    <%= render 'layouts/parts/scripts' %>
</body>
</html>
